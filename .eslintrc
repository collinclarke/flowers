// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "id-length": 0,
    "no-bitwise": "off",
    "no-underscore-dangle": 0,
    "linebreak-style": "off",
    "no-mixed-operators": "off",
    "no-plusplus": [
      0
    ],
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ]
  }
}
